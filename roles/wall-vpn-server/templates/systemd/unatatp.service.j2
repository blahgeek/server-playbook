[Unit]
Description=Userspace NAT is Also a TCP (Transparent) Proxy
After=network-online.target

[Service]
Type=simple
CapabilityBoundingSet=CAP_NET_ADMIN
ExecStart={{ unatatp_binary_path }} -port {{ unatatp.port }}{% if ipv6_prefix is defined %} -bind_ipv6_prefix {{ yikai_net.home_prefix | ansible.utils.ipsubnet(64, vpn_subnet_id + 0x3000) }}{% endif %}

Restart=always
RestartSec=30
StartLimitBurst=10

[Install]
WantedBy=multi-user.target
